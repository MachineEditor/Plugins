{
    "title":"RIFE",
    "formMapping":[
       {
          "containers":[
             "video"
          ],
          "subNav":"filters"
       }
    ],
    "paramMapConfig":{
       "useCurrentPath":true,
       "inputCliParam":"--video",
       "formControls":[
          {
             "cliParam":"--exp",
             "label":"Interpolation",
             "required":false,
             "type":"dropDown",
             "control":{
                "values":[
                   {
                      "text":"2X",
                      "value":1
                   },
                   {
                      "text":"4X",
                      "value":2
                   }
                ],
                "default":1
             }
          },
          {
             "cliParam":"--fps",
             "label":"New video FPS",
             "required":false,
             "type":"input",
             "control":{
                "type":"number"
             }
          }
       ],
       "outputControl":{
          "cliParam":"--out",
          "label":"Output file",
          "required":true,
          "type":"outputFile",
          "control":{
             "extension":"mp4"
          }
       },
       "requireEmptyFolder":true,
       "submitLabel":"Process"
    },
    "processConfig":{
       "loadNewOutputFiles":false,
       "openOutputFolder":true,
       "processOutput":"file",
       "processOutputType":"video",
       "copyDatabaseData":true,
       "notifications":[
          {
             "id":"interpolation",
             "title":"Processing frames",
             "progressOutputParser":"tqdm"
          }
       ]
    },
    "schemaVersion":"1",
    "scriptFilePath":"inference_video.py"
 }